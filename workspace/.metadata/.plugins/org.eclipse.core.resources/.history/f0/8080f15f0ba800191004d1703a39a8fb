package com.mybank.dto;

import java.math.BigDecimal;

public class TransferDTO {


    private Long id;
    private Long idOrigin;
    private Long idDestiny;
    private String accountOrigin;
    private String accountDestiny;
    private BigDecimal ammount;
    private BigDecimal commision;
    private String type;

    public TransferDTO() {
    }

    public TransferDTO(Long id, Long idOrigin, Long idDestiny, String accountOrigin, String accountDestiny,
    		BigDecimal ammount, BigDecimal commision, String type ) {
        this.id = id;
        this.idOrigin = idOrigin;
        this.idDestiny = idDestiny;
        this.accountOrigin = accountOrigin;
        this.accountDestiny= accountDestiny;
        this.ammount = ammount;
        this.commision = commision;
        this.type = type;
    }
    
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

	public Long getIdOrigin() {
		return idOrigin;
	}

	public void setIdOrigin(Long idOrigin) {
		this.idOrigin = idOrigin;
	}

	public Long getIdDestiny() {
		return idDestiny;
	}

	public void setIdDestiny(Long idDestiny) {
		this.idDestiny = idDestiny;
	}
	

	public String getAccountOrigin() {
		return accountOrigin;
	}

	public void setAccountOrigin(String accountOrigin) {
		this.accountOrigin = accountOrigin;
	}

	public String getAccountDestiny() {
		return accountDestiny;
	}

	public void setAccountDestiny(String accountDestiny) {
		this.accountDestiny = accountDestiny;
	}

	public BigDecimal getAmmount() {
		return ammount;
	}

	public void setAmmount(BigDecimal ammount) {
		this.ammount = ammount;
	}

	public BigDecimal getCommision() {
		return commision;
	}

	public void setCommision(BigDecimal commision) {
		this.commision = commision;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	@Override
    public String toString() {
        return "CustomerDTO{" +
                "id=" + id +
                ", origin='" + idOrigin + '\'' +
                ", destiny='" + idDestiny + '\'' +
                ", ammount='" + ammount + '\'' +                
                ", commision='" + commision + '\'' +
                ", type='" + type + '\'' +
                '}';
    }

}
